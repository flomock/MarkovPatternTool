#!/home/go96bix/Dropbox/hiwiManja/MarkovPatternTool/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'MarkovPatternTool','console_scripts','MPT'
__requires__ = 'MarkovPatternTool'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('MarkovPatternTool', 'console_scripts', 'MPT')()
    )
